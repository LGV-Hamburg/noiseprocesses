{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "enum": ["FeatureCollection"]
      },
      "numberReturned": {
        "type": "integer"
      },
      "numberMatched": {
        "type": "integer"
      },
      "timeStamp": {
        "type": "string",
        "format": "date-time"
      },
      "features": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "enum": ["Feature"]
            },
            "id": {
              "type": "integer"
            },
            "geometry": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "enum": ["LineString"]
                },
                "coordinates": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "minItems": 2,
                    "maxItems": 3,
                    "items": [
                      { "type": "number" },
                      { "type": "number" },
                      { "type": "number", "optional": true }
                    ]
                  }
                }
              },
              "required": ["type", "coordinates"]
            },
            "properties": {
              "type": "object",
              "properties": {
                "id": { "type": "integer" },
                "light_speed_day": { "type": "number" },
                "light_speed_evening": { "type": "number" },
                "light_speed_night": { "type": "number" },
                "medium_speed_day": { "type": "number" },
                "medium_speed_evening": { "type": "number" },
                "medium_speed_night": { "type": "number" },
                "heavy_speed_day": { "type": "number" },
                "heavy_speed_evening": { "type": "number" },
                "heavy_speed_night": { "type": "number" },
                "pavement": { "type": "string" }
              },
              "required": ["id", "strassenname", "pavement"]
            }
          },
          "required": ["type", "geometry", "properties"]
        }
      },
      "links": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "href": { "type": "string", "format": "uri" },
            "rel": { "type": "string" },
            "type": { "type": "string" },
            "title": { "type": "string" }
          },
          "required": ["href", "rel", "type"]
        }
      }
    },
    "required": ["type", "features"]
  }